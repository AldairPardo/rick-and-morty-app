  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    },
    "exclude": ["node_modules"],
    "include": [
      "src/**/*",
      "src/schemas/characterSchema.graphql"
    ]
}
